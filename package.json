{
  "name": "Nyota",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "clearCache": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-* && npm cache verify",
    "create-dev-apk": "react-native bundle --dev true --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --reset-cache --assets-dest ./android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "create-prod-apk": "cd android && ./gradlew assembleRelease",
    "create-stg-apk": "cd android && ./gradlew assembleReleaseStaging",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "test": "jest",
    "pretty": "prettier --single-quote --trailing-comma es5 --write '{app,__{tests,mocks}__}/**/*.js'",
    "lint": "eslint .",
    "jest": {
      "preset": "react-native",
      "moduleFileExtensions": [
        "ts",
        "tsx",
        "js"
      ],
      "transform": {
        "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
        "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
      },
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
      "testPathIgnorePatterns": [
        "\\.snap$",
        "<rootDir>/node_modules/"
      ],
      "cacheDirectory": ".jest/cache"
    }
  },
